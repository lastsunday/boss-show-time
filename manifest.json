{
    "manifest_version": 3,
    "name": "Boss Show-time",
    "$schema": "https://json.schemastore.org/chrome-manifest.json",
    "version": "0.1.0",
    "description": "My Chrome Extension",
    "icons": {
        "16": "icons/icon.jpg",
        "32": "icons/icon.jpg",
        "48": "icons/icon.jpg",
        "128": "icons/icon.jpg"
    },
    "content_scripts": [
        {
            "js": ["app.js", "proxyAjax.js"],
            "matches": [
                "https://www.zhipin.com/*"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["proxyAjax.js", "app.css"],
            "matches": ["https://www.zhipin.com/*"]
        }
    ],
    "permissions": [
        "webRequest",
        "https://www.zhipin.com/*"
    ]
}